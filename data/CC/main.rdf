  <rdf:Property rdf:about="http://creativecommons.org/ns#attributionName">
    <rdfs:domain rdf:resource="http://creativecommons.org/ns#Work"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://creativecommons.org/ns#attributionURL">
    <rdfs:domain rdf:resource="http://creativecommons.org/ns#Work"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://creativecommons.org/ns#deprecatedOn">
    <rdfs:domain rdf:resource="http://creativecommons.org/ns#License"/>
    <rdfs:label xml:lang="en-US">deprecated on</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema-datatypes#date"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://creativecommons.org/ns#jurisdiction">
