  <rdf:Description rdf:about="http://web.resource.org/cc/ShareAlike">
    <owl:equivalentClass rdf:resource="http://creativecommons.org/ns#ShareAlike"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://web.resource.org/cc/SourceCode">
    <owl:equivalentClass rdf:resource="http://creativecommons.org/ns#SourceCode"/>
